{
  "name": "dd-gallery",
  "version": "1.0.0",
  "description": "a fullscreen image gallery with a carousel",
  "main": "server/index.js",
  "scripts": {
    "react-dev": "webpack -d --watch",
    "react": "webpack -d",
    "server-dev": "nodemon server/index.js",
    "server": "node server/index.js",
    "test-jest": "jest  --watchAll --detectOpenHandles --forceExit",
    "dataGenerator": "node db/dataGenerator.js",
    "seed-mongo": "bash db/seed/seedMongo.sh",
    "seed-PSQL": "bash db/seed/seedPSQL.sh",
    "postgres-enter": "psql postgres",
    "postgres-start": "pg_ctl -D /usr/local/var/postgres start",
    "postgres-stop": "pg_ctl -D /usr/local/var/postgres stop",
    "pm-start": "pm2 start server/index.js -i max",
    "pm-end": "pm2 stop server/index.js -i max"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeltaDrop/dd-Gallery.git"
  },
  "keywords": [
    "react",
    "gallery",
    "carousel",
    "image",
    "video"
  ],
  "author": "Ben Mangold",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DeltaDrop/dd-Gallery/issues"
  },
  "homepage": "https://github.com/DeltaDrop/dd-Gallery#readme",
  "dependencies": {
    "@newrelic/native-metrics": "^3.1.2",
    "artillery": "^1.6.0-25",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "mongoose": "^5.3.6",
    "newrelic": "^4.11.0",
    "pg": "^7.6.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "sequelize": "^4.41.1",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "pm2": "^3.2.2",
    "request": "^2.88.0",
    "supertest": "^3.3.0",
    "webpack": "^2.7.0"
  }
}
